  "main": "build/index.js",
  "scripts": {
    "clean:server": "rimraf build",
    "clean:client": "rimraf dist",
    "clean": "npm run clean:server && npm run clean:client",
    "build:server": "babel server -d build",
    "build:client": "set NODE_ENV=production && webpack --config ./webpack.production.config.js",
