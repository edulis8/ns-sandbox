    "prod": "set NODE_ENV=production && node build/index.js",
    "dev": "node server/index.babel.js",
    "localstart": "npm run clean && npm run build && npm run prod",
    "start": "node build/index.js",
    "postinstall": "npm run clean && npm run build"
  },
